
(rules PCB NixieDriverBoard
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 1282)
    (layer_rule F.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.3)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.8)
    )
  )
  (rule
    (width 750.0)
    (clear 250.2)
    (clear 375.0 (type smd_to_turn_gap))
    (clear 62.6 (type smd_smd))
  )
  (padstack "Via[0-1]_700:400_um"
    (shape
      (circle F.Cu 700.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 700.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_700:400_um" "Via[0-1]_700:400_um" default
  )
  (via 
    "Via[0-1]_700:400_um-kicad_default" "Via[0-1]_700:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_700:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_700:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 750.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "Net-(74HC1-Pad1)" "Net-(74HC1-Pad2)" "Net-(74HC1-Pad3)" "Net-(74HC1-Pad4)" "Net-(74HC1-Pad5)" "Net-(74HC1-Pad6)" "Net-(74HC1-Pad7)" GND
    "Net-(74HC1-Pad9)" VCC /CLK /PROP /DATA "Net-(74HC1-Pad15)" VDD "Net-(K155ID1-Pad1)"
    "Net-(K155ID1-Pad2)" "Net-(K155ID1-Pad8)" "Net-(K155ID1-Pad9)" "Net-(K155ID1-Pad10)" "Net-(K155ID1-Pad11)" "Net-(K155ID1-Pad13)" "Net-(K155ID1-Pad14)" "Net-(K155ID1-Pad15)"
    "Net-(K155ID1-Pad16)" "Net-(K155ID2-Pad1)" "Net-(K155ID2-Pad2)" "Net-(K155ID2-Pad8)" "Net-(K155ID2-Pad9)" "Net-(K155ID2-Pad10)" "Net-(K155ID2-Pad11)" "Net-(K155ID2-Pad13)"
    "Net-(K155ID2-Pad14)" "Net-(K155ID2-Pad15)" "Net-(K155ID2-Pad16)" "Net-(NIXIE1-Pad1)" "Net-(NIXIE1-Pad12)" "Net-(NIXIE2-Pad1)" "Net-(NIXIE2-Pad12)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 750.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)